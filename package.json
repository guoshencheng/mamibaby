{
  "name": "mamibaby",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"npm:dev:client\" \"npm:dev:server\"",
    "dev:client": "vite --port 3001",
    "dev:server": "nodemon --watch server --ext ts --exec tsx server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "tsc && vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node dist/server/index.js",
    "preview": "npm run build && npm start",
    "pm2:start": "pm2 start ecosystem.config.cjs --env production",
    "pm2:stop": "pm2 stop mamibaby",
    "pm2:restart": "pm2 restart mamibaby",
    "pm2:reload": "pm2 reload mamibaby",
    "pm2:delete": "pm2 delete mamibaby",
    "pm2:logs": "pm2 logs mamibaby",
    "pm2:monit": "pm2 monit",
    "deploy": "npm run build && npm run pm2:reload"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.52",
    "@ai-sdk/openai-compatible": "^1.0.22",
    "@ant-design/x": "^1.6.1",
    "ai": "^5.0.76",
    "antd-mobile": "^5.37.1",
    "antd-mobile-icons": "^0.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "markdown-it": "^14.1.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/markdown-it": "^14.1.2",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
